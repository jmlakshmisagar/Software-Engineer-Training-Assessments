<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="hibernate.practice.model.Student" table="student">
        
        <id name="studentId" column="studentId">
            <generator class="native"/>
        </id>

        <property name="studentName" column="studentName" type="string"/>
        <property name="studentCourse" column="studentCourse" type="string"/>
        <property name="studentMobile" column="studentMobile" type="string"/>
        <property name="studentEmail" column="studentEmail" type="string"/>
        
    </class>
</hibernate-mapping>
